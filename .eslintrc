{
  "plugins": ["prettier"],
  "extends": ["airbnb", "prettier"],
  "rules": {
    "prettier/prettier": "error",
    "react/jsx-filename-extension": 0,
    "jsx-a11y/href-no-hash": 0,
    "react/react-in-jsx-scope": 0,
    "func-names": 0,
    "jsx-a11y/label-has-for": 0,
    "no-undef": 0,
    "new-parens": 0,
    "react/prop-types": 0,
    "no-underscore-dangle": 0,
    "no-nested-ternary": 0
  },
  "parser": "babel-eslint"
}
