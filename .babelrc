{
  "env": {
    "development": {
      "presets": ["next/babel", "@babel/preset-flow"],
      "plugins": [ "inline-react-svg", "transform-flow-strip-types"]
    },
    "production": {
      "presets": ["next/babel"],
      "plugins": [ "inline-react-svg" ]
    },
    "test": {
      "presets": [
        ["env", { "modules": "commonjs" }],
        "next/babel"
      ]
    }
  }
}
