{
  "name": "mechef",
  "version": "0.0.0",
  "description": "A platform where users can cook and sell",
  "main": "index.js",
  "scripts": {
    "start-frontend-server": "node frontend/server.js",
    "lint": "eslint ./",
    "serve": "nodemon api/app.js",
    "start": "debug=true node api/app.js",
    "build": "next build ./frontend/",
    "start-prod": "NODE_ENV=production node frontend/server.js",
    "flow": "flow check",
    "test": "debug=false NODE_ENV=test jest",
    "test-update": "debug=false NODE_ENV=test jest -u",
    "size": "bundlesize",
    "prettier:all": "prettier --write \"./**/*.js\""
  },
  "bundlesize": [
    {
      "path": "./frontend/.next/static/**/dashboard.js",
      "compression": "none",
      "maxSize": "170 KB"
    }
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mechef/mechef.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mechef/mechef/issues"
  },
  "homepage": "https://github.com/mechef/mechef/#readme",
  "dependencies": {
    "apidoc": "0.17.6",
    "babel-core": "7.0.0-bridge.0",
    "bluebird": "3.5.1",
    "body-parser": "1.15.0",
    "cookie-parser": "^1.4.3",
    "form-urlencoded": "^2.0.4",
    "gridfs-stream": "1.1.1",
    "i18next": "^10.6.0",
    "i18next-browser-languagedetector": "^2.2.3",
    "i18next-express-middleware": "^1.4.0",
    "i18next-node-fs-backend": "^1.2.1",
    "i18next-xhr-backend": "^1.5.1",
    "isomorphic-unfetch": "^2.1.1",
    "jsonwebtoken": "7.4.1",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "mongoose": "4.11.1",
    "multer": "1.3.0",
    "nodemailer": "^4.6.8",
    "react": "^16.5.2",
    "react-dates": "^17.2.0",
    "react-dom": "^16.5.2",
    "react-geosuggest": "^2.8.0",
    "react-i18next": "^7.12.0",
    "react-media": "^1.8.0",
    "recompose": "^0.28.2",
    "rxjs": "^5.5.12",
    "styled-jsx": "2.2.7",
    "uuid": "3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/plugin-transform-flow-strip-types": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.6.0",
    "babel-plugin-inline-react-svg": "^0.5.0",
    "bundlesize": "^0.17.0",
    "eslint": "^4.4.0",
    "eslint-config-prettier": "^2.10.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.11.1",
    "express": "^4.16.3",
    "flow-bin": "^0.80.0",
    "husky": "^1.0.0-rc.15",
    "jest": "^21.2.1",
    "lint-staged": "^7.3.0",
    "next": "^7.0.0",
    "prettier": "^1.14.3",
    "react-test-renderer": "^16.5.2"
  }
}
